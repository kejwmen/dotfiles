! Rofi
rofi.color-normal: #1d1f21, #ffffff, #1d1f21, #ffffff, #1d1f21
rofi.color-urgent: #1d1f21, #cc6666, #1d1f21, #cc6666, #1d1f21
rofi.color-active: #1d1f21, #81a2be, #1d1f21, #81a2be, #1d1f21
rofi.color-window: #1d1f21, #ffffff
rofi.width: 100
rofi.padding: 449
rofi.fullscreen: true
rofi.bw: 0
rofi.lines: 10
rofi.opacity: 90

! URxvt

URxvt.perl-ext-common: default,matcher,tabbedex,clipboard,selection-to-clipboard
! Look
URxvt*font: xft:Source\ Code\ Pro\ Medium:pixelsize=16:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=16:antialias=true:hinting=true
URxvt*letterSpace: -1
URxvt*scrollBar: false
URxvt.transparent:   true
URxvt.shading:       10
! Feel
URxvt.iso14755: False
URxvt.iso14755_52: False
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~
URxvt.keysym.M-Down:        \033[1;3B
URxvt.keysym.M-Up:          \033[1;3A
URxvt.keysym.M-Left:        \033[1;3D
URxvt.keysym.M-Right:       \033[1;3C
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
! Scrolling
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
URxvt.secondaryWheel: 1
! URL launcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
! Tabs
URxvt.tabbed*transparent: true
URxvt.tabbed.new-button: false
URxvt.tabbed.autohide: true
URxvt.tabbed.title: false
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 7
URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 0

URxvt.keysym.Control-t: perl:tabbedex:new_tab
URxvt.keysym.Control-Tab: perl:tabbedex:next_tab
URxvt.keysym.Control-Shift-Tab: perl:tabbedex:prev_tab

